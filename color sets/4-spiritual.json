{
    "name": "Devyasa Dark Spiritual",
    "colors": {
        "editor.background": "#1A1C1A", // Eerie Black
        "editor.foreground": "#F0EAD6", // Divine White
        "activityBarBadge.background": "#5DADE2", // Tranquil Blue
        "sideBarTitle.foreground": "#D4AC0D" // Harmony Beige
    },
    "tokenColors": [
        {
            "name": "Comment",
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#796681" // Sacred Violet
            }
        },
        {
            "name": "Variables",
            "scope": [
                "variable",
                "string constant.other.placeholder"
            ],
            "settings": {
                "foreground": "#F0EAD6" // Divine White
            }
        },
        {
            "name": "Invalid",
            "scope": [
                "invalid",
                "invalid.illegal"
            ],
            "settings": {
                "foreground": "#E74C3C" // Soft Red
            }
        },
        {
            "name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
            "scope": [
                "constant.numeric",
                "constant.language",
                "support.constant",
                "constant.character",
                "constant.escape",
                "variable.parameter",
                "keyword.other.unit",
                "keyword.other"
            ],
            "settings": {
                "foreground": "#58D68D" // Peaceful Green
            }
        },
        {
            "name": "String, Symbols, Inherited Class, Markup Heading",
            "scope": [
                "string",
                "constant.other.symbol",
                "constant.other.key",
                "entity.other.inherited-class",
                "markup.heading",
                "markup.inserted.git_gutter",
                "meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
            ],
            "settings": {
                "foreground": "#e9a362" // Harmony Beige
            }
        },
        {
            "name": "Tag",
            "scope": [
                "entity.name.tag",
                "meta.tag.sgml",
                "markup.deleted.git_gutter"
            ],
            "settings": {
                "foreground": "#ff758f" // Soft Red
            }
        },
        {
            "name": "Function, Special Method",
            "scope": [
                "entity.name.function",
                "meta.function-call",
                "variable.function",
                "support.function",
                "keyword.other.special-method"
            ],
            "settings": {
                "foreground": "#1ABC9C" // Serene Teal
            }
        },
        {
            "name": "CSS Class and Support",
            "scope": [
                "source.css support.type.property-name",
                "source.sass support.type.property-name",
                "source.scss support.type.property-name",
                "source.less support.type.property-name",
                "source.stylus support.type.property-name",
                "source.postcss support.type.property-name"
            ],
            "settings": {
                "foreground": "#5DADE2" // Tranquil Blue
            }
        },
        {
            "name": "Decorators",
            "scope": [
                "tag.decorator.js entity.name.tag.js",
                "tag.decorator.js punctuation.definition.tag.js"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#9B59B6" // Sacred Violet
            }
        },
        {
            "name": "JSON Key - Level 0",
            "scope": [
                "source.json meta.structure.dictionary.json support.type.property-name.json"
            ],
            "settings": {
                "foreground": "#F0EAD6" // Divine White
            }
        },
        {
            "name": "JSON Key - Level 1",
            "scope": [
                "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
            ],
            "settings": {
                "foreground": "#5DADE2" // Tranquil Blue
            }
        },
        {
            "name": "JSON Key - Level 2",
            "scope": [
                "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
            ],
            "settings": {
                "foreground": "#58D68D" // Peaceful Green
            }
        },
        {
            "name": "JSON Key - Level 3",
            "scope": [
                "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.value.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
            ],
            "settings": {
                "foreground": "#9B59B6" // Sacred Violet
            }
        },
        {
            "name": "Attributes",
            "scope": [
                "entity.other.attribute-name"
            ],
            "settings": {
                "foreground": "#be95c4" // Sacred Violet
            }
        },
        {
            "name": "HTML Attributes",
            "scope": [
                "text.html.basic entity.other.attribute-name.html",
                "text.html.basic entity.other.attribute-name"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#5DADE2" // Tranquil Blue
            }
        },
        {
            "name": "Markdown - Plain",
            "scope": [
                "text.html.markdown",
                "punctuation.definition.list_item.markdown"
            ],
            "settings": {
                "foreground": "#F0EAD6" // Divine White
            }
        },
        {
            "name": "Markdown - Markup Raw Inline",
            "scope": [
                "text.html.markdown markup.inline.raw.markdown"
            ],
            "settings": {
                "foreground": "#E74C3C" // Soft Red
            }
        },
        {
            "name": "Markdown - Markup Raw Inline Punctuation",
            "scope": [
                "text.html.markdown markup.inline.raw.markdown punctuation.definition.raw.markdown"
            ],
            "settings": {
                "foreground": "#8c8c8c" // Light grey for punctuation
            }
        },
        {
            "name": "Markdown - Heading",
            "scope": [
                "markdown.heading",
                "markup.heading | markup.heading entity.name",
                "markup.heading.markdown punctuation.definition.heading.markdown"
            ],
            "settings": {
                "foreground": "#58D68D" // Peaceful Green for markdown headings
            }
        },
        {
            "name": "Markup - Italic",
            "scope": [
                "markup.italic"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#D4AC0D" // Harmony Beige for italic
            }
        },
        {
            "name": "Markup - Bold",
            "scope": [
                "markup.bold",
                "markup.bold string"
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#D4AC0D" // Harmony Beige for bold
            }
        },
        {
            "name": "Markup - Bold-Italic",
            "scope": [
                "markup.bold markup.italic",
                "markup.italic markup.bold",
                "markup.quote markup.bold",
                "markup.bold markup.italic string",
                "markup.italic markup.bold string",
                "markup.quote markup.bold string"
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#D4AC0D" // Harmony Beige for bold-italic
            }
        },
        {
            "name": "Markup - Underline",
            "scope": [
                "markup.underline"
            ],
            "settings": {
                "fontStyle": "underline",
                "foreground": "#1ABC9C" // Serene Teal for underline
            }
        },
        {
            "name": "Markdown - Blockquote",
            "scope": [
                "markup.quote punctuation.definition.blockquote.markdown"
            ],
            "settings": {
                "foreground": "#8c8c8c" // Light grey for blockquote
            }
        },
        {
            "name": "Markup - Quote",
            "scope": [
                "markup.quote"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#9B59B6" // Sacred Violet for quote
            }
        },
        {
            "name": "Markdown - Link",
            "scope": [
                "string.other.link.title.markdown"
            ],
            "settings": {
                "foreground": "#5DADE2" // Tranquil Blue for link
            }
        },
        {
            "name": "Markdown - Link Description",
            "scope": [
                "string.other.link.description.title.markdown"
            ],
            "settings": {
                "foreground": "#58D68D" // Peaceful Green for link description
            }
        },
        {
            "name": "Markdown - Link Anchor",
            "scope": [
                "constant.other.reference.link.markdown"
            ],
            "settings": {
                "foreground": "#9B59B6" // Sacred Violet for link anchor
            }
        },
        {
            "name": "Markup - Raw Block",
            "scope": [
                "markup.raw.block"
            ],
            "settings": {
                "foreground": "#5DADE2" // Tranquil Blue for raw block
            }
        },
        {
            "name": "Markdown - Raw Block Fenced",
            "scope": [
                "markup.raw.block.fenced.markdown"
            ],
            "settings": {
                "foreground": "#00000050" // Darker grey for fenced block
            }
        },
        {
            "name": "Markdown - Fenced Code Block",
            "scope": [
                "punctuation.definition.fenced.markdown"
            ],
            "settings": {
                "foreground": "#00000050" // Darker grey for fenced code block
            }
        },
        {
            "name": "Markdown - Fenced Code Block Variable",
            "scope": [
                "markup.raw.block.fenced.markdown",
                "variable.language.fenced.markdown",
                "punctuation.section.class.end"
            ],
            "settings": {
                "foreground": "#E74C3C" // Soft Red for fenced code block variable
            }
        },
        {
            "name": "Markdown - Fenced Language",
            "scope": [
                "variable.language.fenced.markdown"
            ],
            "settings": {
                "foreground": "#9B59B6" // Sacred Violet for fenced language
            }
        },
        {
            "name": "Markdown - Separator",
            "scope": [
                "meta.separator"
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#8c8c8c" // Light grey for separator
            }
        },
        {
            "name": "Markup - Table",
            "scope": [
                "markup.table"
            ],
            "settings": {
                "foreground": "#F0EAD6" // Divine White for table
            }
        },
        {
            "name": "URL",
            "scope": [
                "*url*",
                "*link*",
                "*uri*"
            ],
            "settings": {
                "fontStyle": "underline",
                "foreground": "#5DADE2" // Tranquil Blue for URLs
            }
        },
        {
            "name": "Regular Expressions",
            "scope": [
                "string.regexp"
            ],
            "settings": {
                "foreground": "#58D68D" // Peaceful Green for regex
            }
        },
        {
            "name": "Escape Characters",
            "scope": [
                "constant.character.escape"
            ],
            "settings": {
                "foreground": "#9B59B6" // Sacred Violet for escape characters
            }
        },
        {
            "name": "Changed",
            "scope": [
                "markup.changed"
            ],
            "settings": {
                "foreground": "#E74C3C" // Soft Red for changes
            }
        },
        {
            "name": "Inserted",
            "scope": [
                "markup.inserted"
            ],
            "settings": {
                "foreground": "#58D68D" // Peaceful Green for insertions
            }
        },
        {
            "name": "Deleted",
            "scope": [
                "markup.deleted"
            ],
            "settings": {
                "foreground": "#E74C3C" // Soft Red for deletions
            }
        },
        {
            "name": "Language methods",
            "scope": [
                "variable.language"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#5DADE2" // Tranquil Blue for language methods
            }
        },
        {
            "name": "entity.name.method.js",
            "scope": [
                "entity.name.method.js"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#58D68D" // Peaceful Green for JS methods
            }
        },
        {
            "name": "meta.method.js",
            "scope": [
                "meta.class-method.js entity.name.function.js",
                "variable.function.constructor"
            ],
            "settings": {
                "foreground": "#58D68D" // Peaceful Green for JS methods
            }
        },
        {
            "name": "Attribute Values",
            "scope": [
                "variable.other.readwrite.alias.tsx",
                "meta.import.tsx",
                "source.tsx"
            ],
            "settings": {
                "foreground": "#F0EAD6"
            } // Divine White
        },
        {
            "name": "TypeScript React Import Keywords",
            "scope": [
                "keyword.control.import.tsx",
                "meta.import.tsx",
                "source.tsx"
            ],
            "settings": {
                "foreground": "#cfb929" // Divine White
            }
        },
        {
            "name": "TypeScript React Storage Type",
            "scope": [
                "storage.type.tsx"
            ],
            "settings": {
                "foreground": "#a3a8fd" // Divine White
            }
        },
        {
            "name": "TypeScript React Other Tokens",
            "scope": [
                "meta.var.expr.tsx",
                "meta.arrow.tsx",
                "source.tsx"
            ],
            "settings": {
                "foreground": "#fae6ea" // Divine White
            }
        },
        {
            "name": "JSX Tags",
            "scope": [
                "meta.jsx.children.tsx",
                "meta.tag.without-attributes.tsx",
                "meta.tag.tsx",
                "meta.arrow.tsx",
                "meta.var.expr.tsx",
                "source.tsx"
            ],
            "settings": {
                "foreground": "#FCF7EB" // Light Beige
            }
        },
        {
            "name": "JSX Block",
            "scope": [
                "meta.block.tsx"
            ],
            "settings": {
                "foreground": "#f05f5f" // Light Beige for meta.block.tsx
            }
        },
        {
            "name": "TypeScript React Strings",
            "scope": [
                "string.quoted.double.tsx",
                "meta.tag.attributes.tsx",
                "meta.tag.tsx",
                "meta.function.expression.tsx",
                "meta.export.default.tsx",
                "source.tsx"
            ],
            "settings": {
                "foreground": "#d8b28e" // Soft Orange
            }
        }
    ]
}