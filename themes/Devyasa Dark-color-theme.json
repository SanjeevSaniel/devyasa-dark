{
  "name": "Devyasa Dark",
  "colors": {
    "editor.background": "#1A1C1A", // Editor background
    "editor.foreground": "#F0EAD6", // Editor foreground
    "activityBar.background": "#223355", // Navbar background
    "activityBarBadge.background": "#5DADE2", // Navbar badge background
    "sideBar.background": "#1c201c", // Sidebar background
    "sideBarTitle.foreground": "#D4AC0D", // Sidebar title foreground
    "statusBar.background": "#223355", // Status bar background
    "titleBar.activeBackground": "#223355", // Title bar background
    "panel.background": "#1A1C1A", // Panel background
    "editorGroupHeader.tabsBackground": "#1A1C1A", // Tabs background
    "tab.activeBackground": "#1A1C1A", // Active tab background
    "tab.inactiveBackground": "#2A2C2A", // Inactive tab background (optional)
    "terminal.background": "#1A1C1A",
    "terminal.foreground": "#F0EAD6",
    "terminal.ansiBlack": "#000000",
    "terminal.ansiRed": "#FF5555",
    "terminal.ansiGreen": "#50FA7B",
    "terminal.ansiYellow": "#F1FA8C",
    "terminal.ansiBlue": "#BD93F9",
    "terminal.ansiMagenta": "#FF79C6",
    "terminal.ansiCyan": "#8BE9FD",
    "terminal.ansiWhite": "#BFBFBF",
    "terminal.ansiBrightBlack": "#4D4D4D",
    "terminal.ansiBrightRed": "#FF6E6E",
    "terminal.ansiBrightGreen": "#69FF94",
    "terminal.ansiBrightYellow": "#FFFFA5",
    "terminal.ansiBrightBlue": "#D6ACFF",
    "terminal.ansiBrightMagenta": "#FF92D0",
    "terminal.ansiBrightCyan": "#A4FFFF",
    "terminal.ansiBrightWhite": "#E6E6E6"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#796681" // Sacred Violet
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "string constant.other.placeholder",
        "variable.other.object.js",
        "meta.var.expr.js",
        "source.js",
        "variable.other.object.jsx",
        "meta.var.expr.jsx",
        "source.jsx",
        "variable.other.object.ts",
        "meta.var.expr.ts",
        "source.ts",
        "variable.other.object.tsx",
        "meta.var.expr.tsx",
        "source.tsx"
      ],
      "settings": {
        "foreground": "#79bcf3" // Divine White
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#E74C3C" // Soft Red
      }
    },
    {
      "name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
      "scope": [
        "constant.numeric",
        "constant.language",
        "support.constant",
        "constant.character",
        "constant.escape",
        "variable.parameter",
        "keyword.other.unit",
        "keyword.other"
      ],
      "settings": {
        "foreground": "#58D68D" // Peaceful Green
      }
    },
    {
      "name": "String, Symbols, Inherited Class, Markup Heading",
      "scope": [
        "string",
        "constant.other.symbol",
        "constant.other.key",
        "entity.other.inherited-class",
        "markup.heading",
        "markup.inserted.git_gutter",
        "meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js",
        "string.quoted.double.tsx",
        "meta.tag.attributes.tsx",
        "meta.tag.tsx",
        "meta.function.expression.tsx",
        "meta.export.default.tsx",
        "source.tsx"
      ],
      "settings": {
        "foreground": "#d8b28e" // Soft Orange
      }
    },
    {
      "name": "Tag",
      "scope": [
        "entity.name.tag",
        "meta.tag.sgml",
        "markup.deleted.git_gutter"
      ],
      "settings": {
        "foreground": "#ff758f" // Soft Red
      }
    },
    {
      "name": "Function, Special Method",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "variable.function",
        "support.function",
        "keyword.other.special-method"
      ],
      "settings": {
        "foreground": "#1ABC9C" // Serene Teal
      }
    },
    {
      "name": "CSS Class and Support",
      "scope": [
        "source.css support.type.property-name",
        "source.sass support.type.property-name",
        "source.scss support.type.property-name",
        "source.less support.type.property-name",
        "source.stylus support.type.property-name",
        "source.postcss support.type.property-name"
      ],
      "settings": {
        "foreground": "#5DADE2" // Tranquil Blue
      }
    },
    {
      "name": "Decorators",
      "scope": [
        "tag.decorator.js entity.name.tag.js",
        "tag.decorator.js punctuation.definition.tag.js"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#9B59B6" // Sacred Violet
      }
    },
    {
      "name": "JSON Key - Level 0",
      "scope": [
        "source.json meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#F0EAD6" // Divine White
      }
    },
    {
      "name": "JSON Key - Level 1",
      "scope": [
        "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#5DADE2" // Tranquil Blue
      }
    },
    {
      "name": "JSON Key - Level 2",
      "scope": [
        "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#58D68D" // Peaceful Green
      }
    },
    {
      "name": "JSON Key - Level 3",
      "scope": [
        "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.value.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#9B59B6" // Sacred Violet
      }
    },
    {
      "name": "Attributes",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#be95c4" // Sacred Violet
      }
    },
    {
      "name": "HTML Attributes",
      "scope": [
        "text.html.basic entity.other.attribute-name.html",
        "text.html.basic entity.other.attribute-name"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#5DADE2" // Tranquil Blue
      }
    },
    {
      "name": "Markdown - Plain",
      "scope": [
        "text.html.markdown",
        "punctuation.definition.list_item.markdown"
      ],
      "settings": {
        "foreground": "#F0EAD6" // Divine White
      }
    },
    {
      "name": "Markdown - Markup Raw Inline",
      "scope": ["text.html.markdown markup.inline.raw.markdown"],
      "settings": {
        "foreground": "#E74C3C" // Soft Red
      }
    },
    {
      "name": "Markdown - Markup Raw Inline Punctuation",
      "scope": [
        "text.html.markdown markup.inline.raw.markdown punctuation.definition.raw.markdown"
      ],
      "settings": {
        "foreground": "#8c8c8c" // Light grey for punctuation
      }
    },
    {
      "name": "Markdown - Heading",
      "scope": [
        "markdown.heading",
        "markup.heading | markup.heading entity.name",
        "markup.heading.markdown punctuation.definition.heading.markdown"
      ],
      "settings": {
        "foreground": "#58D68D" // Peaceful Green for markdown headings
      }
    },
    {
      "name": "Markup - Italic",
      "scope": ["markup.italic"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#D4AC0D" // Harmony Beige for italic
      }
    },
    {
      "name": "Markup - Bold",
      "scope": ["markup.bold", "markup.bold string"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#D4AC0D" // Harmony Beige for bold
      }
    },
    {
      "name": "Markup - Bold-Italic",
      "scope": [
        "markup.bold markup.italic",
        "markup.italic markup.bold",
        "markup.quote markup.bold",
        "markup.bold markup.italic string",
        "markup.italic markup.bold string",
        "markup.quote markup.bold string"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#D4AC0D" // Harmony Beige for bold-italic
      }
    },
    {
      "name": "Markup - Underline",
      "scope": ["markup.underline"],
      "settings": {
        "fontStyle": "underline",
        "foreground": "#1ABC9C" // Serene Teal for underline
      }
    },
    {
      "name": "Markdown - Blockquote",
      "scope": ["markup.quote punctuation.definition.blockquote.markdown"],
      "settings": {
        "foreground": "#8c8c8c" // Light grey for blockquote
      }
    },
    {
      "name": "Markup - Quote",
      "scope": ["markup.quote"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#9B59B6" // Sacred Violet for quote
      }
    },
    {
      "name": "Markdown - Link",
      "scope": ["string.other.link.title.markdown"],
      "settings": {
        "foreground": "#5DADE2" // Tranquil Blue for link
      }
    },
    {
      "name": "Markdown - Link Description",
      "scope": ["string.other.link.description.title.markdown"],
      "settings": {
        "foreground": "#58D68D" // Peaceful Green for link description
      }
    },
    {
      "name": "Markdown - Link Anchor",
      "scope": ["constant.other.reference.link.markdown"],
      "settings": {
        "foreground": "#9B59B6" // Sacred Violet for link anchor
      }
    },
    {
      "name": "Markup - Raw Block",
      "scope": ["markup.raw.block"],
      "settings": {
        "foreground": "#5DADE2" // Tranquil Blue for raw block
      }
    },
    {
      "name": "Markdown - Raw Block Fenced",
      "scope": ["markup.raw.block.fenced.markdown"],
      "settings": {
        "foreground": "#00000050" // Darker grey for fenced block
      }
    },
    {
      "name": "Markdown - Fenced Code Block",
      "scope": ["punctuation.definition.fenced.markdown"],
      "settings": {
        "foreground": "#00000050" // Darker grey for fenced code block
      }
    },
    {
      "name": "Markdown - Fenced Code Block Variable",
      "scope": [
        "markup.raw.block.fenced.markdown",
        "variable.language.fenced.markdown",
        "punctuation.section.class.end"
      ],
      "settings": {
        "foreground": "#E74C3C" // Soft Red for fenced code block variable
      }
    },
    {
      "name": "Markdown - Fenced Language",
      "scope": ["variable.language.fenced.markdown"],
      "settings": {
        "foreground": "#9B59B6" // Sacred Violet for fenced language
      }
    },
    {
      "name": "Markdown - Separator",
      "scope": ["meta.separator"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#8c8c8c" // Light grey for separator
      }
    },
    {
      "name": "Markup - Table",
      "scope": ["markup.table"],
      "settings": {
        "foreground": "#F0EAD6" // Divine White for table
      }
    },
    {
      "name": "URL",
      "scope": ["*url*", "*link*", "*uri*"],
      "settings": {
        "fontStyle": "underline",
        "foreground": "#5DADE2" // Tranquil Blue for URLs
      }
    },
    {
      "name": "Regular Expressions",
      "scope": ["string.regexp"],
      "settings": {
        "foreground": "#58D68D" // Peaceful Green for regex
      }
    },
    {
      "name": "Escape Characters",
      "scope": ["constant.character.escape"],
      "settings": {
        "foreground": "#9B59B6" // Sacred Violet for escape characters
      }
    },
    {
      "name": "Changed",
      "scope": ["markup.changed"],
      "settings": {
        "foreground": "#E74C3C" // Soft Red for changes
      }
    },
    {
      "name": "Inserted",
      "scope": ["markup.inserted"],
      "settings": {
        "foreground": "#58D68D" // Peaceful Green for insertions
      }
    },
    {
      "name": "Deleted",
      "scope": ["markup.deleted"],
      "settings": {
        "foreground": "#E74C3C" // Soft Red for deletions
      }
    },
    {
      "name": "Language methods",
      "scope": ["variable.language"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#5DADE2" // Tranquil Blue for language methods
      }
    },
    {
      "name": "Methods",
      "scope": [
        "entity.name.method.js",
        "entity.name.method.jsx",
        "entity.name.method.ts",
        "entity.name.method.tsx",
        "meta.class-method.js entity.name.function.js",
        "meta.class-method.jsx entity.name.function.jsx",
        "meta.class-method.ts entity.name.function.ts",
        "meta.class-method.tsx entity.name.function.tsx",
        "variable.function.constructor.js",
        "variable.function.constructor.jsx",
        "variable.function.constructor.ts",
        "variable.function.constructor.tsx"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#58D68D" // Peaceful Green for methods
      }
    },
    {
      "name": "Attribute Values",
      "scope": [
        "variable.other.readwrite.alias.tsx",
        "meta.import.tsx",
        "source.tsx",
        "variable.other.readwrite.alias.ts",
        "meta.import.ts",
        "source.ts",
        "variable.other.readwrite.alias.jsx",
        "meta.import.jsx",
        "source.jsx",
        "variable.other.readwrite.alias.js",
        "meta.import.js",
        "source.js"
      ],
      "settings": {
        "foreground": "#F0EAD6" // Divine White
      }
    },
    {
      "name": "Import Keywords",
      "scope": [
        "keyword.control.import.js",
        "meta.import.js",
        "source.js",
        "keyword.control.import.jsx",
        "meta.import.jsx",
        "source.jsx",
        "keyword.control.import.tsx",
        "meta.import.tsx",
        "source.tsx",
        "keyword.control.import.ts",
        "meta.import.ts",
        "source.ts"
      ],
      "settings": {
        "foreground": "#cfb929" // Divine White
      }
    },
    {
      "name": "Storage Type",
      "scope": [
        "storage.type.js",
        "storage.type.jsx",
        "storage.type.tsx",
        "storage.type.ts"
      ],
      "settings": {
        "foreground": "#a3a8fd" // Divine White
      }
    },
    {
      "name": "Other Tokens",
      "scope": [
        "meta.var.expr.tsx",
        "meta.arrow.tsx",
        "source.tsx",
        "meta.var.expr.jsx",
        "meta.arrow.jsx",
        "source.jsx",
        "meta.var.expr.ts",
        "meta.arrow.ts",
        "source.ts",
        "meta.var.expr.js",
        "meta.arrow.js",
        "source.js"
      ],
      "settings": {
        "foreground": "#fae6ea" // Divine White
      }
    },
    {
      "name": "JSX Tags",
      "scope": [
        "meta.jsx.children.tsx",
        "meta.tag.without-attributes.tsx",
        "meta.tag.tsx",
        "meta.arrow.tsx",
        "meta.var.expr.tsx",
        "source.tsx",
        "meta.jsx.children.jsx",
        "meta.tag.without-attributes.jsx",
        "meta.tag.jsx",
        "meta.arrow.jsx",
        "meta.var.expr.jsx",
        "source.jsx"
      ],
      "settings": {
        "foreground": "#FCF7EB" // Light Beige
      }
    },
    {
      "name": "JSX Block",
      "scope": ["meta.block.tsx", "meta.block.jsx"],
      "settings": {
        "foreground": "#f05f5f" // Light Beige for meta.block
      }
    },
    {
      "name": "Built-in Class",
      "scope": [
        "support.class.builtin.js",
        "support.class.builtin.jsx",
        "support.class.builtin.ts",
        "support.class.builtin.tsx"
      ],
      "settings": {
        "foreground": "#ff688b" // Divine White
      }
    },
    {
      "name": "JavaScript Tokens",
      "scope": [
        "meta.object.member.js",
        "meta.objectliteral.js",
        "new.expr.js",
        "meta.var.expr.js",
        "source.js",
        "meta.object.member.jsx",
        "meta.objectliteral.jsx",
        "new.expr.jsx",
        "meta.var.expr.jsx",
        "source.jsx",
        "meta.object.member.ts",
        "meta.objectliteral.ts",
        "new.expr.ts",
        "meta.var.expr.ts",
        "source.ts",
        "meta.object.member.tsx",
        "meta.objectliteral.tsx",
        "new.expr.tsx",
        "meta.var.expr.tsx",
        "source.tsx"
      ],
      "settings": {
        "foreground": "#F0EAD6" // Divine White
      }
    },
    {
      "name": "Tailwind CSS At-rule Media Header",
      "scope": ["meta.at-rule.media.header.css"],
      "settings": {
        "foreground": "#da98f5"
      }
    },
    {
      "name": "Tailwind CSS Source",
      "scope": ["source.css.tailwind"],
      "settings": {
        "foreground": "#fa8dc3"
      }
    }
  ]
}
